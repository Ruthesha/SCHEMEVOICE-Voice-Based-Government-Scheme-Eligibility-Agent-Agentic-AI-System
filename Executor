def executor(action: str, memory: Dict) -> Dict:

    if action == "CHECK_ELIGIBILITY":
        print("[EXECUTOR] Starting eligibility check")

        # Tool1:Eligibility check
        eligible_schemes = eligibility_tool(
            memory["age"],
            memory["income"],
            memory["gender"],
            memory["is_widow"]
        )

        # Tool2:Fetching scheme details
        detailed_info = {}
        for scheme in eligible_schemes:
            info = scheme_info_tool(scheme)
            if info:
                detailed_info[scheme] = info

        return {
            "eligible_schemes": eligible_schemes,
            "scheme_details": detailed_info
        }

    return None
