def eligibility_tool(age: int, income: int, gender: str, is_widow: Optional[bool]) -> List[str]:
    schemes = []

    if age >= 60 and income <= 200000:
        schemes.append("వృద్ధాప్య పెన్షన్")

    if age <= 25 and income <= 300000:
        schemes.append("విద్యార్థి స్కాలర్‌షిప్")

    if gender == "female" and is_widow:
        schemes.append("వితంతు పెన్షన్")

    return schemes
